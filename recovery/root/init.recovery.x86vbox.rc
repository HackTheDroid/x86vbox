on init
    exec -- /bin/sh /sbin/init.x86vbox.sh

service network_start /sbin/network_start.sh
    user root
    seclabel u:r:recovery:s0
    oneshot

service console /bin/sh
    class core
    console
#    disabled
#    user shell
    group shell log
    seclabel u:r:shell:s0

on property:ro.debuggable=1
    start console
